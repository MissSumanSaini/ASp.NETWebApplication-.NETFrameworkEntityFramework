


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Employee</title>
    <table class= "table table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>MobileNo</th>
                <th>Image</th>
                <th>Salary</th> 
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var emp in Model)
            { 
            <tr>
                <td>@emp.ID</td>
                <td>@emp.Name</td>
                <td>@emp.MobileNo</td>
                <td>@emp.Image</td>
                <td>@emp.Salary</td>
                <td>@emp.Email</td>
            </tr>
            }
            </tbody>
        </table>

</head>
<body>
    <div> 
    </div>
</body>
</html>
